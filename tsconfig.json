{
	"compilerOptions": {
	  "target": "es6",
	  "module": "commonjs",
	  "rootDir": "./src",
	  "outDir": "./build",
	  "sourceMap": true,
	  "removeComments": true,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "strict": true,
	  "noImplicitAny": true,
	  "skipLibCheck": true
	},
}
  